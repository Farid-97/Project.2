<body class="back">
  

<div class="wrapper">
  <div class="bg"> Cards collection </div>
  <div class="fg"> Cards collection </div>
</div>
<section class="organize">
 
  {{#each cardsCollection}}
  <div class="card">
    <div class="image">
      <img class="front"
        src="{{imageCard}}"
      />
    </div>
    <div class="details">
      <div class="center">
        <h1 class="inside">{{name}}<br /><span class="insides">{{team}}</span></h1>
        <p class="p">{{shirtNumber}}</p>
        <ul class="ul">
          <li class="li"><a class="a" href="#"><i
                class="fa {{!-- fa-facebook --}}"
                aria-hidden="true"
              ></i></a></li>
        </ul>
      </div>
    </div>
  </div>
  {{/each}}
</section>
</body>